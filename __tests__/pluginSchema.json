{
  "$ref": "genericSchema.json",
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "Root Package.json Schema",
  "description": "Schema for all Package.json files in Grouparoo.",
  "required": ["publishConfig"],
  "properties": {
    "publishConfig": {
      "$id": "#/properties/publishConfig",
      "type": "object",
      "examples": [{ "access": "public" }],
      "required": ["access"],
      "properties": {
        "access": {
          "$id": "#/properties/publishConfig/access",
          "type": "string",
          "examples": ["public"]
        }
      }
    }
  }
}
