{
  "id": "email_group",
  "name": "People with Email Addresses",
  "class": "Group",
  "type": "calculated",
  "rules": [
    {
      "propertyId": "user_id",
      "operation": { "op": "exists" }
    },
    {
      "propertyId": "email",
      "operation": { "op": "like" },
      "match": "%@%"
    }
  ]
}
