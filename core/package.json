  "repository": {
    "type": "git",
    "url": "https://github.com/grouparoo/grouparoo.git",
    "directory": "core"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "main": "api/dist/index",
  "types": "api/dist/index",
  "scripts": {
    "prepare": "npm run build-api && npm run build-web && npm run thank-you",
    "build-api": "cd api && rm -rf dist && tsc --declaration",
    "build-web": "cd web && next build",
    "start": "./api/bin/start",
    "dev": "./api/bin/dev",
    "test": "npm run test-api && npm run test-web",
    "pretest": "npm run lint && npm run prepare",
    "test-api": "cd api && jest",
    "test-web": "cd web && jest",
    "lint": "npm run lint-api && npm run lint-web",
    "lint-api": "cd api && prettier --check src __tests__",
    "lint-web": "cd web && prettier --check pages components classes client utils hooks __tests__",
    "docs": "cd api && typedoc --out docs --theme default --mode file --exclude \"src/actions/**/*.ts\" --exclude \"src/tasks/**/*.ts\"",
    "import": "./api/bin/import",
    "thank-you": "./thank-you"
  },
  "dependencies": {
