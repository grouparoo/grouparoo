{
  "packages": [
    "cli",
    "apps/*",
    "core",
    "clients/@grouparoo/*",
    "plugins/@grouparoo/*",
    "plugins/@grouparoo-inc/*"
  ],
  "version": "0.1.12-alpha.0",
  "command": {
    "bootstrap": {
      "ignore": "@grouparoo/app-local*"
    },
    "version": {
      "allowBranch": [
        "master",
        "stable"
      ],
      "message": "chore(release): publish %s [ci skip]"
    }
  }
}
