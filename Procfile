worker: cd /app/apps/$GROUPAROO_MONOREPO_APP/node_modules/@grouparoo/core && INIT_CWD="/app/apps/$GROUPAROO_MONOREPO_APP" WEB_SERVER=false WORKERS=5 ./bin/start
